<Application x:Class="LiveCardBrowser.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LiveCardBrowser"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BooleanToVisibilityConverter x:Key="BoolToVis"/>
        <local:VisToBool x:Key="VisibleIfTrue" Inverted="True" Not="False" />
        <local:VisToBool x:Key="VisibleIfNotTrue" Inverted="True" Not="True" />
    </Application.Resources>
</Application>
