{
	"$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
	"type": "AdaptiveCard",
	"version": "1.0",
	"body": [
		{
			"type": "Container",
			"items": [
				{
					"type": "TextBlock",
					"text": "{companyName} ({primaryExchange}: {symbol})",
					"size": "Medium",
					"isSubtle": true
				},
				{
					"type": "TextBlock",
					"text": "{latestUpdateFormatted}",
					"isSubtle": true
				}
			]
		},
		{
			"type": "Container",
			"spacing": "None",
			"items": [
				{
					"type": "ColumnSet",
					"columns": [
						{
							"type": "Column",
							"width": "stretch",
							"items": [
								{
									"type": "TextBlock",
									"text": "{latestPrice}",
									"size": "ExtraLarge"
								},
								{
									"type": "TextBlock",
									"text": "{changeFormatted}",
									"color": "{changeColor}",
									"spacing": "None"
								}
							]
						},
						{
							"type": "Column",
							"width": "auto",
							"items": [
								{
									"type": "FactSet",
									"facts": [
										{
											"title": "Open",
											"value": "{open}"
										},
										{
											"title": "High",
											"value": "{high}"
										},
										{
											"title": "Low",
											"value": "{low}"
										}
									]
								}
							]
						}
					]
				}
			]
		},
		{
			"type": "TextBlock",
			"text": "Data provided for free by [IEX](https://iextrading.com/developer). View [IEXâ€™s Terms of Use](https://iextrading.com/api-exhibit-a/)",
			"size": "Small",
			"isSubtle": true
		}
	]
}
