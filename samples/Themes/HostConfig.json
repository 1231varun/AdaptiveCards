    "BackgroundColor": "#FFFFFF",
    "FontFamily": "Calibri",
    "ActionsOrientation": "horizontal",
    "ActionAlignment": "center",
    "SupportsInteractivity": true,
    "SupportedActionTypes": [
      "Action.OpenUrl",
      "Action.Submit",
      "Action.Http",
      "Action.ShowCard"
