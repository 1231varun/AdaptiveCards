    "Accent": {
      "Normal": "#FF0000FF",
      "Subtle": "#b20000FF"
    },
    "Dark": {
      "Normal": "#FF101010",
      "Subtle": "#b2101010"
    },
    "Light": {
      "Normal": "#FFFFFFFF",
      "Subtle": "#b2FFFFFF"
    },
    "Good": {
      "Normal": "#FF008000",
      "Subtle": "#b2008000"
    },
    "Warning": {
      "Normal": "#FFFFD700",
      "Subtle": "#b2FFD700"
    },
    "Attention": {
      "Normal": "#FF8B0000",
      "Subtle": "#b28B0000"
    }
  },
  "TextBlock": {
